MIRROR=/home/cjh/tmp/mirror/
REPO=/home/cjh/work/VistA-Document-Library/
wget --directory-prefix=$MIRROR -m -w 1 -N --user-agent="Mozilla/5.0" "http://www.va.gov/vdl/application.asp?appid=5" --no-parent
. /home/cjh/workspace/vdl-repo-tools/src/test.sh
rm -rf /home/cjh/work/VistA-Document-Library/*
cd /home/cjh/workspace/vdl-repo-tools/src
python vdl-repo-generator.py -m $MIRROR -o $REPO
python vdl-repo-commit.py -r $REPO
